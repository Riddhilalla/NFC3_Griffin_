    <style>
        /* Sidebar styles */
        .sidebar {
            width: 65px;
            height: 100vh;
            position: fixed;
            top: 0;
            left: 0;
            background-color: #3A5A40;
            color: #DAD7CD;
            transition: width 0.3s;
            overflow-x: hidden;
            z-index: 1000;
        }

        .sidebar:hover {
            width: 300px;
        }

        .sidebar .nav-link {
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        .main-content {
            margin-left: 60px; /* Margin for sidebar */
            transition: margin-left 0.3s;
        }

        .sidebar:hover + .main-content {
            margin-left: 300px; /* Adjust margin when sidebar is expanded */
        }
        .nav-link .text{
            color: #DAD7CD;
        }
        .nav-link svg {
            margin-right: 8px; /* Space between icon and text */
        }
        .left{
            padding: 30px;
        }
        
        .details {
            width: 500px;
        }
        .about{
            width: 500px;
        }
        .about a{
            text-decoration: none;
        }
        .card-body{
            font-family: 'Open Sans', Times, serif;
            text-decoration: none;
            color: #3A5A40;
        }
        .graph{
            width: 800px;
    position: relative;
    top: -731px;
    right: -454px;
        }
        .schedule{
            position: relative;
    bottom: 822px;
    left: 250px;
        }
        .btn {
            background-color: #3A5A40;
            color: #DAD7CD;
            font-family: 'Poppins', Courier, monospace;
            font-weight: 400;
        }

        .calendar {
            display: grid;
            grid-template-columns: repeat(7, 1fr);
            gap: 10px;
            text-align: center;
            

        }

        .calendar .day {
            padding: 15px;
            background-color: #DAD7CD;
            border-radius: 5px;
        }

        .calendar .day.event {
            background-color: #3A5A40;
            color: white;
        }

        #bsb-chart-1{
            color: #3A5A40;
            border: none;
        }
        .fc-direction-ltr .fc-toolbar > * > :not(:first-child){
            color: #DAD7CD;
            background-color: #3A5A40;
            border: none;
        }
        .btn-group button{
            color: #DAD7CD;
            border: none;
        }
        .fc, .fc *, .fc ::after, .fc ::before {
            font-family: 'Poppins', Courier, monospace;
            font-weight: 500;
            color: #3A5A40;
            border: none;
        }
        .btn-group>.btn-group:not(:last-child)>.btn, .btn-group>.btn.dropdown-toggle-split:first-child, .btn-group>.btn:not(:last-child):not(.dropdown-toggle) {
            color: #DAD7CD;
            background-color: #3A5A40;
            border: none;
        }
        .fc-h-event .fc-event-main {
            background-color: #3A5A40;
            color: #DAD7CD;
        }


    </style>
