           <script src="https://cdn.jsdelivr.net/npm/fullcalendar@6.1.9/index.global.min.js"></script>
           <script src="https://cdn.jsdelivr.net/npm/@fullcalendar/bootstrap5@6.1.9/index.global.min.js"></script>
           <script src="https://unpkg.com/bs-brain@2.0.4/components/calendars/calendar-1/assets/controller/calendar-1.js"></script>
         
           <script>
             document.addEventListener('DOMContentLoaded', function () {
               // Initialize the FullCalendar with desired configurations
               var calendarEl = document.getElementById('bsb-calendar-1');
         
               var calendar = new FullCalendar.Calendar(calendarEl, {
                 themeSystem: 'bootstrap5', // Use Bootstrap 5 theme
                 initialView: 'dayGridMonth', // Set initial view to month grid
                 headerToolbar: { // Toolbar configuration
                   left: 'prev,next today',
                   center: 'title',
                   right: 'dayGridMonth,timeGridWeek,timeGridDay'
                 },
                 events: [ // Add recent events
                   {
                     title: 'Medical Check-up',
                     start: '2024-08-28', // Event start date
                     description: 'Routine medical check-up for all staff members.'
                   },
                   {
                     title: 'Surgery Follow-up',
                     start: '2024-08-30',
                     description: 'Follow-up appointment for post-surgery patients.'
                   },
                   {
                     title: 'Community Health Workshop',
                     start: '2024-09-05',
                     description: 'Workshop on community health awareness and practices.'
                   }
                 ],
                 eventClick: function (info) { // Functionality to handle event clicks
                   // Display an alert or any other UI element to show event details
                   alert('Event: ' + info.event.title + '\n' + 'Description: ' + info.event.extendedProps.description);
                 },
                 editable: true, // Allows editing events
                 selectable: true // Allows selecting date ranges
               });
         
               calendar.render(); // Render the calendar
             });
           </script>
           <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
